{
  "name": "ngx-cookie-service",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build NgxCookieService",
    "build:watch": "ng build NgxCookieService --watch",
    "build:package": "ng build NgxCookieService",
    "test": "ng test NgxCookieService --watch=false --coverage",
    "test:watch": "ng test NgxCookieService --watch=true --coverage",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage/ngx-cookie-service",
    "release:bug": "npm version patch --prefix ./library/ngx-cookie-service",
    "release:feature": "npm version minor --prefix ./library/ngx-cookie-service",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "search:port:pid": "lsof -n -i4TCP:8080",
    "start": "ng serve --project=TestCases --host=0.0.0.0",
    "build:testcases": "ng build --project=TestCases --prod",
    "publish": "cd dist/ngx-cookie-service && npm publish --access public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.0.14",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/router": "~10.0.14",
    "core-js": "^2.5.4",
    "ngx-markdown": "^7.1.5",
    "rxjs": "~6.6.2",
    "stickyfilljs": "^2.1.0",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.4.2",
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~10.0.8",
    "@angular/compiler-cli": "~10.0.14",
    "@angular/language-service": "~10.0.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "generate-changelog": "^1.8.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.7.1",
    "jest-junit": "^6.3.0",
    "jest-junit-reporter": "^1.1.0",
    "ng-packagr": "^10.1.0",
    "node-sass": "^4.14.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
